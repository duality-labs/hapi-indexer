
NODE_ENV=production

# Database file path
DB_FILENAME=/tmp/database.db

# Chain endpoints
REST_API=https://lcd.dev.duality.xyz
RPC_API=https://rpc.dev.duality.xyz
WEBSOCKET_URL=wss://rpc.dev.duality.xyz/websocket

# Chain registry asset data
CHAIN_REGISTRY_CHAIN_NAME=

# Coin price data
COIN_GECKO_DEMO_API_KEY=
COIN_GECKO_PRO_API_KEY=

